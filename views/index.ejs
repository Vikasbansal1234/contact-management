<!DOCTYPE html>
<html ng-app="contactApp">
<head lang="en">
    <meta charset="UTF-8">
    <title></title>
    <link rel="stylesheet" href="stylesheets/bootstrap.min.css??ver=<%=cache['bootstrap.min.css']%>"/>
    <style>
        body{
            font-family: serif;
        }
    </style>
</head>
<body ng-controller="MainCtrl" ng-cloak>
<div class="container-fluid">
    <div class="navbar navbar-default navbar-static-top">
        <div class="navbar-header">
            <a href="#" class="navbar-brand">
                Contact Management System
            </a>
        </div>
        <div>
            <a type="button"  href="/logout" class="btn btn-link navbar-btn navbar-right" ng-click="logout()" ng-show="loggedUser">LOGOUT</a>
            <p class="navbar-text navbar-right" ng-show="loggedUser">Welcome {{loggedUser.name}} </p>
            <button type="button" class="btn btn-link navbar-btn navbar-right" ng-click="login()" ng-hide="loggedUser">LOGIN</button>
            <button type="button" class="btn btn-link navbar-btn navbar-right" ng-click="signup()" ng-hide="loggedUser">SIGNUP</button>
        </div>
    </div>



    <div class="ui-view">
        <img src="images/Global%20MarketingIAV.jpg" height="550px;" width="100%"/>
    </div>
</div>

<script src="javascripts/common/jquery-1.7.2.min.js?ver=<%=cache['jquery-1.7.2.min.js']%>"></script>
<script src="javascripts/common/angular.min.js?ver=<%=cache['angular.min.js']%>"></script>
<script src="javascripts/common/angular-ui-router.js?ver=<%=cache['angular-ui-router']%>"></script>
<script src="javascripts/common/bootstrap.min.js?ver=<%=cache['bootstrap.min.js']%>"></script>
<script src="javascripts/common/ui-bootstrap-tpls-0.11.2.min.js?ver=<%=cache['config.js']%>"></script>
<script>
    var contactApp=angular.module('contactApp',['ui.router','ui.bootstrap']);
</script>
<script src="javascripts/controller/admincontroller.js?ver=<%=cache['admincontroller.js']%>"></script>
<script src="javascripts/controller/contactcontroller.js?ver=<%=cache['contactcontroller.js']%>"></script>
<script src="javascripts/controller/logincontroller.js?ver=<%=cache['logincontroller.js']%>"></script>
<script src="javascripts/controller/signupcontroller.js?ver=<%=cache['signupcontroller.js']%>"></script>
<script src="javascripts/controller/maincontroller.js?ver=<%=cache['maincontroller.js']%>"></script>
<script src="javascripts/controller/alertcontroller.js?ver=<%=cache['alertcontroller.js']%>"></script>
<script src="javascripts/factory/factory.js?ver=<%=cache['factory.js']%>"></script>
<script src="javascripts/config/config.js?ver=<%=cache['config.js']%>"></script>

</body>
</html>